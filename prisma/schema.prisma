// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Vehicle {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())

  manufacturer String
  model        String
  year         Int
  engineSize   Int
  registration String
  colour       String
  fuelType     String
  bodyType     String

  ownerId String

  @@index([ownerId])
}
